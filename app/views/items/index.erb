<h4>Welcome, <%=@user.username%> - <a href="/users/<%=@user.username%>">My Items</a> - <a href="/logout">Logout</a> </h4>

<h2>All Items Available</h2>
<% Item.all.each do |i| %>
<ul>
	<li><a href="/items/<%=i.id%>"><%=i.name%></a>, Quantity: <%=i.quantity%>, from <a href="/user/<%=i.user.username%>"><%=i.user.username%></a></li>
</ul>
<%end%>